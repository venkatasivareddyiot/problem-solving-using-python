    "def closestZero(li):\n",
    "    #seperate all numbers less than zero\n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    \n",
    "    n1=[]\n",
    "    for i in li:\n",
    "        if i<0:\n",
    "            n1.append(i)\n",
    "    pl=[]\n",
    "    for i in li:\n",
    "        if i >=0:\n",
    "            pl.append(i)\n",
    "    nc=max(n1)\n",
    "    pc=min(pl)\n",
    "    if abs(nc) == abs(pc):\n",
    "        return pc\n",
    "    elif abs(nc)<pc:\n",
    "        return abs(nc)\n",
    "    else:\n",
    "        return pc\n",
    "closestZero(li)"
